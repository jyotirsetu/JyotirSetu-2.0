{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    }
  },
  "include": [".astro/types.d.ts", "src/**/*"],
  "exclude": [
    "dist/", 
    ".vercel/", 
    "node_modules/",
    "**/.vercel/**",
    "**/dist/**"
  ]
}
